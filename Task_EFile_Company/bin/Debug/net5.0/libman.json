{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-icons@1.5.0",
      "destination": "wwwroot/lib/bootstrap-icons/",
      "files": [
        "bootstrap-icons.svg",
        "font/bootstrap-icons.min.css",
        "font/fonts/bootstrap-icons.woff"
      ]
    },
    {
      "library": "datatable@2.0.1",
      "destination": "wwwroot/lib/datatable/"
    },
    {
      "library": "sweetalert@2.1.2",
      "destination": "wwwroot/lib/sweetalert/"
    },
    {
      "library": "signalr.js@2.4.2",
      "destination": "wwwroot/lib/signalr.js/"
    },
    {
      "library": "microsoft-signalr@5.0.9",
      "destination": "wwwroot/lib/microsoft-signalr/",
      "files": [
        "signalr.min.js"
      ]
    }
  ]
}